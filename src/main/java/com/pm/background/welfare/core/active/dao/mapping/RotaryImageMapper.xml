<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pm.background.welfare.core.active.dao.RotaryImageMapper">
    <update id="ifExhibitionImage" parameterType="com.pm.background.welfare.core.active.entity.RotaryImage">
        update rotary_image set
        status = #{status},
        statusName = #{statusName}
        where id = #{id}
    </update>

</mapper>